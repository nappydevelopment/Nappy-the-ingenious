<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="gdpRoot"
		  fx:controller="nappydevelopment.nappy_the_ingenious.gui.fxml.AddCharacterController" 
	      stylesheets="/nappydevelopment/nappy_the_ingenious/gui/fxml/AddCharacterCSS.css" 
	      hgap="10.0"
	      maxHeight="-Infinity"
	      maxWidth="-Infinity"
	      minHeight="-Infinity"
	      minWidth="-Infinity"
	      prefHeight="395.0"
	      prefWidth="531.0"
	      vgap="10.0"
	      xmlns:fx="http://javafx.com/fxml/1"
	      xmlns="http://javafx.com/javafx/8.0.65">
  
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="300.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="312.0" minWidth="10.0" prefWidth="300.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Button fx:id="btnAbort" onAction="#btnAbortOnAction" mnemonicParsing="false" text="Abbrechen" GridPane.rowIndex="2" />
      <Button fx:id="btnNext" onAction="#btnNextOnAction" mnemonicParsing="false" text="Weiter" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label text="Vorname:" />
      <Label text="Nachname:" GridPane.rowIndex="1" />
      <TextField GridPane.columnIndex="1" GridPane.rowIndex="0" />
      <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" />
   </children>
   <padding>
      <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
   </padding>
</GridPane>